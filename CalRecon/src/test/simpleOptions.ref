Reconstruction.Members += { "TkrReconAlg/Iteration" } ;
PropertiesCheckAlg.targets += { "CalXtalRecAlg" } ;
PropertiesCheckAlg.targets += { "first", "FirstPass", "second", "Iteration" } ;




// <-- End of including file $CALRECONROOT/src/test/reconstruction.txt




