// event loop
ApplicationMgr.DLLs += { "GlastSvc" } ;
ApplicationMgr.ExtSvc = { "GlastEventSelector/EventSelector" } ;
EventSelector.Input = "NONE" ;
ApplicationMgr.ExtSvc += { "EventCnvSvc" } ;
EventPersistencySvc.CnvServices = { "EventCnvSvc" } ;
ApplicationMgr.EvtMax = 20 ;

// geometry
ApplicationMgr.DLLs += { "G4Generator","G4Propagator", "TkrUtil", "CalUtil" } ;
ApplicationMgr.ExtSvc += { "GlastDetSvc", "G4GeometrySvc" } ;
GlastDetSvc.topVolume = "LAT" ; 
GlastDetSvc.xmlfile = "$(XMLGEODBSROOT)/xml/flight/flightSegVols.xml" ;
GlastDetSvc.visitorMode = "recon" ;

// calibration services
ApplicationMgr.DLLs +={ "CalibSvc" } ;
ApplicationMgr.ExtSvc += {"CalibDataSvc"} ;
ApplicationMgr.ExtSvc += {"CalibMySQLCnvSvc", "CalibXmlCnvSvc" } ;
DetectorPersistencySvc.CnvServices += { "CalibMySQLCnvSvc" } ;
DetectorPersistencySvc.CnvServices += { "CalibXmlCnvSvc" } ;
CalibDataSvc.CalibInstrumentName = "LAT" ;
CalibDataSvc.CalibStorageType = 14 ; // Mysql
CalibDataSvc.CalibRootName = "Calib" ;
CalibDataSvc.CalibFlavorList = {"ideal"} ;
ApplicationMgr.Dlls += {"CalXtalResponse"} ;
ApplicationMgr.ExtSvc += {"CalCalibSvc"} ;
CalCalibSvc.DefaultFlavor  = "ideal" ;

// algorithms skeleton
ApplicationMgr.DLLs += { "GaudiAlg", "RootIo" } ;
ApplicationMgr.TopAlg = { "PropertiesCheckAlg" } ;  
ApplicationMgr.TopAlg += { "TkrCalibAlg" } ;
ApplicationMgr.TopAlg += { "Sequencer/Simulation" } ;
ApplicationMgr.TopAlg += { "Sequencer/Reconstruction" } ;
MessageSvc.OutputLevel = 3 ;


// <-- End of including file $CALRECONROOT/src/test/common.txt


// read back simulation and reconstruction first pass
Simulation.Members += { "digiRootReaderAlg", "reconRootReaderAlg" } ;  
PropertiesCheckAlg.targets += { "digiRootReaderAlg", "reconRootReaderAlg"  } ;
digiRootReaderAlg.digiRootFile = "$CALRECONROOT/src/test/gamma_5_gev_normal-digi.root" ;
reconRootReaderAlg.reconRootFile = "$CALRECONROOT/src/test/gamma_5_gev_normal-recon.root" ;

// calo reconstruction second pass
ApplicationMgr.DLLs += { "CalRecon" } ;
ApplicationMgr.ExtSvc += { "GlastPropagatorSvc" } ;
Reconstruction.Members += { "CalClustersAlg" } ;
PropertiesCheckAlg.targets += { "CalClustersAlg" } ;
ApplicationMgr.TopAlg += { "test_CalRecon" } ;




// <-- End of including file $CALRECONROOT/src/test/jobOptions.txt

CalClustersAlg.clusteringToolName="CalSimpleClusteringTool";

