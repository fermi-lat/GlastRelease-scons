package calibUtil
version v0r0

use xml v4r* 
use MYSQLEXT v0r*
# use ROOT

set MYSQL_HOST centaurusa.slac.stanford.edu
set MYSQL_METATABLE metadata_v1
## when we've written an actual public header file, uncomment the following:
apply_pattern package_include
apply_pattern package_headers

## Start with static library, but ultimately should be shareable
## when there is some c or c++ source, uncomment the following
## library calibUtil -no_share  $(calibUtil_headers) *.cxx *.c


library calibUtil -no_share $(calibUtil_headers) *.cxx *.c


application client test/main.cxx StripSrv.cxx GenericSrv.cxx















