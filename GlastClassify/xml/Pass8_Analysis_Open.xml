      <ActivityNode engineClass="com.insightful.miner.CreateColumnsEngineNode" guiClass="com.insightful.miner.CreateColumnsNodeModel" id="1" numInputs="1" numOutputs="1">
        <DisplayInfo labelText="(1) SimpleBestEnergy" largeIcon="create_cols_large.gif" smallIcon="create_cols_small.gif" x="232" y="74"/>
        <ArgumentList>
          <XTProps>
            <Property name="newColumns">
              <Property name="SimpleBestEnergy">
                <Property name="dataType" value="Double_t"/>
                <Property name="expression" value="( (CalNSaturated&gt;0)*(CalCfpEnergyUB)+(CalNSaturated==0)*((CalCfpEnergyUB&gt;0)*CalCfpEnergyUB + (CalCfpEnergyUB&lt;=0)*((EvtEnergyCorrUB&gt;0)*EvtEnergyCorrUB + (EvtEnergyCorrUB&lt;=0)*((CalCfpEnergy&gt;0)*CalCfpEnergy + (CalCfpEnergy&lt;=0)*EvtEnergyCorr))) )"/>
                <Property name="type" value="continuous"/>
              </Property>
              <Property name="SimpleBestLogEnergy">
                <Property name="dataType" value="Double_t"/>
                <Property name="expression" value="log10( (CalNSaturated&gt;0)*(CalCfpEnergyUB)+(CalNSaturated==0)*((CalCfpEnergyUB&gt;0)*CalCfpEnergyUB + (CalCfpEnergyUB&lt;=0)*((EvtEnergyCorrUB&gt;0)*EvtEnergyCorrUB + (EvtEnergyCorrUB&lt;=0)*((CalCfpEnergy&gt;0)*CalCfpEnergy + (CalCfpEnergy&lt;=0)*EvtEnergyCorr))) )"/>
                <Property name="type" value="continuous"/>
              </Property>
              <Property name="SimpleBestEnergy">
                <Property name="dataType" value="Double_t"/>
                <Property name="expression" value="( (CalNSaturated&gt;0)*(CalCfpEnergyUB)+(CalNSaturated==0)*((CalCfpEnergyUB&gt;0)*CalCfpEnergyUB + (CalCfpEnergyUB&lt;=0)*((EvtEnergyCorrUB&gt;0)*EvtEnergyCorrUB + (EvtEnergyCorrUB&lt;=0)*((CalCfpEnergy&gt;0)*CalCfpEnergy + (CalCfpEnergy&lt;=0)*EvtEnergyCorr))) )"/>
                <Property name="type" value="continuous"/>
              </Property>
              <Property name="SimpleBestLogEnergy">
                <Property name="dataType" value="Double_t"/>
                <Property name="expression" value="log10( (CalNSaturated&gt;0)*(CalCfpEnergyUB)+(CalNSaturated==0)*((CalCfpEnergyUB&gt;0)*CalCfpEnergyUB + (CalCfpEnergyUB&lt;=0)*((EvtEnergyCorrUB&gt;0)*EvtEnergyCorrUB + (EvtEnergyCorrUB&lt;=0)*((CalCfpEnergy&gt;0)*CalCfpEnergy + (CalCfpEnergy&lt;=0)*EvtEnergyCorr))) )"/>
                <Property name="type" value="continuous"/>
              </Property>
            </Property>
          </XTProps>
        </ArgumentList>
      </ActivityNode>
      <ActivityNode engineClass="com.insightful.miner.CreateColumnsEngineNode" guiClass="com.insightful.miner.CreateColumnsNodeModel" id="2" numInputs="1" numOutputs="1">
        <DisplayInfo labelText="(2) ACD Variables" largeIcon="create_cols_large.gif" smallIcon="create_cols_small.gif" x="381" y="74"/>
        <ArgumentList>
          <XTProps>
            <Property name="newColumns">
              <Property name="Acd2Tkr1VetoSigma">
                <Property name="dataType" value="Double_t"/>
                <Property name="expression" value="sqrt( (Acd2Tkr1VetoSigmaMip)^2 + ( SimpleBestLogEnergy * 1/10.*Acd2Tkr1VetoSigmaProj)^2)"/>
                <Property name="type" value="continuous"/>
              </Property>
              <Property name="Acd2TriggerTileEnergyRatio">
                <Property name="dataType" value="Double_t"/>
                <Property name="expression" value="Acd2TriggerTileEnergy/max(10., SimpleBestEnergy) * 100"/>
                <Property name="type" value="continuous"/>
              </Property>
            </Property>
          </XTProps>
        </ArgumentList>
      </ActivityNode>
      <ActivityNode engineClass="com.insightful.miner.CutSetEngineNode" guiClass="com.insightful.miner.CutSetNodeModel" id="3" numInputs="1" numOutputs="1">
        <DisplayInfo labelText="(3) ACD Prefilters" largeIcon="cutset_large.gif" smallIcon="cutset_small.gif" x="516" y="75"/>
        <ArgumentList>
          <XTProps>
            <Property name="cutDefinitions">
              <Property name="Cal1ConeVeto">
                <Property name="cutValue" value="!( Acd2Cal1Energy15 &gt; max(0.75,10/3.*SimpleBestLogEnergy - 13.4) )"/>
                <Property name="makeOutput" value="false"/>
              </Property>
              <Property name="Tkr1ConeVeto">
                <Property name="cutValue" value="!( Acd2Tkr1Energy30 &gt; max(0.5,27/4.*SimpleBestLogEnergy - 24.8) )"/>
                <Property name="makeOutput" value="false"/>
              </Property>
              <Property name="Tkr1GapVeto">
                <Property name="cutValue" value="!( (Tkr1SSDVeto == 0 & Acd2Tkr1VetoSigmaGap &lt; 2. ) | (Tkr1SSDVeto &lt; 3 & abs(Acd2Tkr1CornerDoca) &lt; 5.) )"/>
                <Property name="makeOutput" value="false"/>
              </Property>
              <Property name="Tkr1SigmaVeto">
                <Property name="cutValue" value="!( Acd2Tkr1VetoSigma &lt; 1.5 | (Tkr1SSDVeto &lt; 2 & Acd2Tkr1VetoSigma &lt; 3.) )"/>
                <Property name="makeOutput" value="false"/>
              </Property>
              <Property name="TriggerTileEnergyVeto">
                <Property name="cutValue" value="!( Acd2TriggerTileEnergyRatio &gt; 0.8 )"/>
                <Property name="makeOutput" value="false"/>
              </Property>
            </Property>
            <Property name="cutSetName" value="AcdPrefilters"/>
          </XTProps>
        </ArgumentList>
      </ActivityNode>
